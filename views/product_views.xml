<odoo>
    <record id="view_product_form_inherit_library" model="ir.ui.view">
        <field name="name">product.template.form.inherit.library</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='list_price']" position="after">
                <field name="is_membership_product" string=" Membership Product"/>
            </xpath>
        </field>
    </record>
</odoo>
